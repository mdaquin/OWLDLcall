        <call:functionToCall rdf:resource="https://k.loria.fr/ontologies/examples/equations#findObviousRootEqDeg2"/>
        <call:hasParams rdf:resource="https://k.loria.fr/ontologies/examples/equations#paramsCallQuadEquationSolving"/>
        <call:domain rdf:resource="https://k.loria.fr/ontologies/examples/equations#QuadraticEquationSolving"/>
        <call:range rdf:resource="https://k.loria.fr/ontologies/examples/equations#SolutionSet"/>
        <call:subsumingProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasSolutionSet"/>
    </owl:NamedIndividual>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#callIsAPolynomialEquation -->

    <owl:NamedIndividual rdf:about="https://k.loria.fr/ontologies/examples/equations#callIsAPolynomialEquation">
        <rdf:type rdf:resource="https://k.loria.fr/ontologies/call#CallFormula"/>
        <call:functionToCall rdf:resource="https://k.loria.fr/ontologies/examples/equations#funIsAPolynomialEquation"/>
        <call:hasParams rdf:resource="https://k.loria.fr/ontologies/examples/equations#paramsCallDegree"/>
        <call:domain rdf:resource="https://k.loria.fr/ontologies/examples/equations#Equation1RealUnknown"/>
        <call:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <call:subsumingProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#isAPolynomialEquation"/>
    </owl:NamedIndividual>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#callUseDiscriminant -->

    <owl:NamedIndividual rdf:about="https://k.loria.fr/ontologies/examples/equations#callUseDiscriminant">
        <rdf:type rdf:resource="https://k.loria.fr/ontologies/call#CallFormula"/>
        <call:functionToCall rdf:resource="https://k.loria.fr/ontologies/examples/equations#useDiscriminant"/>
        <call:hasParams rdf:resource="https://k.loria.fr/ontologies/examples/equations#paramsCallQuadEquationSolving"/>
        <call:domain rdf:resource="https://k.loria.fr/ontologies/examples/equations#QuadraticEquationSolving"/>
        <call:range rdf:resource="https://k.loria.fr/ontologies/examples/equations#SolutionSet"/>
        <call:subsumingProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasSolutionSet"/>
