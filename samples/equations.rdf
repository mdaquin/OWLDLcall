    <owl:NamedIndividual rdf:about="https://k.loria.fr/ontologies/examples/equations#findObviousRootEqDeg2">
        <isPreferredTo rdf:resource="https://k.loria.fr/ontologies/examples/equations#useDiscriminant"/>
        <call:hasPyExec>def poly_obvious_roots(unk_name: str, equality: str):
    import sympy as sp

    unk = sp.symbols(unk_name)
    eq_str = equality.split(&quot;=&quot;)
    if len(eq_str) != 2:
        return None
    eq = sp.Eq(sp.sympify(eq_str[0]), sp.sympify(eq_str[1])).simplify().as_poly()
    for value in range(-2, 3):
        subst_expr = eq.subs(unk, value)
        res = subst_expr.evalf()
        # if we found an obvious root
        if res == 0:
            root1 = value
            coeffs = eq.all_coeffs()
            root2 = -(coeffs[1]/coeffs[0])-root1
            # negative values mess up the order of sets, so I do it manually
            result = {root1, root2} if root1 &lt;= root2 else {root2, root1}
            
            return 0, [(0, &quot;hasValue&quot;, str(result))]
    return None</call:hasPyExec>
        <call:hasPyExpr>poly_obvious_roots</call:hasPyExpr>
    </owl:NamedIndividual>
    


