        <call:hasFunctionCall rdf:resource="https://k.loria.fr/ontologies/examples/equations#funCallFindObviousRootEqDeg2List"/>
        <call:domain rdf:resource="https://k.loria.fr/ontologies/examples/equations#Equation2ndDegree"/>
        <call:range rdf:resource="https://k.loria.fr/ontologies/examples/equations#SolutionSet"/>
        <call:subsumingProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasSolutionSet"/>
