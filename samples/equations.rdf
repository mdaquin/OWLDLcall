        <call:functionToCall rdf:resource="https://k.loria.fr/ontologies/examples/equations#funFindEigenValues"/>
        <call:hasParams rdf:resource="https://k.loria.fr/ontologies/examples/equations#paramsCallFindEigenValues"/>
        <call:domain rdf:resource="https://k.loria.fr/ontologies/examples/equations#FindingEigenValues"/>
        <call:range rdf:resource="https://k.loria.fr/ontologies/examples/equations#EquationSolving"/>
        <call:subsumingProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasSubProblem"/>
