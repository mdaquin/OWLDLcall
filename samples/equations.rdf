<?xml version="1.0"?>
<rdf:RDF xmlns="https://k.loria.fr/ontologies/examples/equations#"
     xml:base="https://k.loria.fr/ontologies/examples/equations"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:call="https://k.loria.fr/ontologies/call#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="https://k.loria.fr/ontologies/examples/equations">
        <owl:imports rdf:resource="https://k.loria.fr/ontologies/call"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://k.loria.fr/ontologies/examples/equations#hasCharacteristicPolynomial -->

    <owl:ObjectProperty rdf:about="https://k.loria.fr/ontologies/examples/equations#hasCharacteristicPolynomial">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="https://k.loria.fr/ontologies/examples/equations#Polynomial"/>
    </owl:ObjectProperty>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#hasEquation -->

    <owl:ObjectProperty rdf:about="https://k.loria.fr/ontologies/examples/equations#hasEquation">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#hasRootSet -->

    <owl:ObjectProperty rdf:about="https://k.loria.fr/ontologies/examples/equations#hasRootSet">
        <rdfs:range rdf:resource="https://k.loria.fr/ontologies/examples/equations#SolutionSet"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://k.loria.fr/ontologies/examples/equations#hasEquation"/>
            <rdf:Description rdf:about="https://k.loria.fr/ontologies/examples/equations#hasSolutionSet"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#hasSolution -->

    <owl:ObjectProperty rdf:about="https://k.loria.fr/ontologies/examples/equations#hasSolution">
        <rdfs:domain rdf:resource="https://k.loria.fr/ontologies/examples/equations#Problem"/>
        <rdfs:range rdf:resource="https://k.loria.fr/ontologies/examples/equations#Solution"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://k.loria.fr/ontologies/examples/equations#hasEquation"/>
            <rdf:Description rdf:about="https://k.loria.fr/ontologies/examples/equations#hasSolutionSet"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#hasSolutionSet -->

    <owl:ObjectProperty rdf:about="https://k.loria.fr/ontologies/examples/equations#hasSolutionSet">
        <rdfs:domain rdf:resource="https://k.loria.fr/ontologies/examples/equations#Equation2ndDegree"/>
        <rdfs:range rdf:resource="https://k.loria.fr/ontologies/examples/equations#SolutionSet"/>
    </owl:ObjectProperty>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#hasSolvingMethod -->

    <owl:ObjectProperty rdf:about="https://k.loria.fr/ontologies/examples/equations#hasSolvingMethod"/>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#hasUnknown -->

    <owl:ObjectProperty rdf:about="https://k.loria.fr/ontologies/examples/equations#hasUnknown">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="https://k.loria.fr/ontologies/examples/equations#Unknown"/>
    </owl:ObjectProperty>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#isPreferredTo -->

    <owl:ObjectProperty rdf:about="https://k.loria.fr/ontologies/examples/equations#isPreferredTo"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://k.loria.fr/ontologies/examples/equations#degree -->

    <owl:DatatypeProperty rdf:about="https://k.loria.fr/ontologies/examples/equations#degree">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://k.loria.fr/ontologies/examples/equations#PolynomialEquation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#hasEquality -->

    <owl:DatatypeProperty rdf:about="https://k.loria.fr/ontologies/examples/equations#hasEquality">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#hasExpression -->

    <owl:DatatypeProperty rdf:about="https://k.loria.fr/ontologies/examples/equations#hasExpression">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://k.loria.fr/ontologies/examples/equations#Matrix"/>
                    <rdf:Description rdf:about="https://k.loria.fr/ontologies/examples/equations#Polynomial"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#hasName -->

    <owl:DatatypeProperty rdf:about="https://k.loria.fr/ontologies/examples/equations#hasName">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#hasRoot -->

    <owl:DatatypeProperty rdf:about="https://k.loria.fr/ontologies/examples/equations#hasRoot">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#hasType -->

    <owl:DatatypeProperty rdf:about="https://k.loria.fr/ontologies/examples/equations#hasType">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#isAPolynomialEquation -->

    <owl:DatatypeProperty rdf:about="https://k.loria.fr/ontologies/examples/equations#isAPolynomialEquation">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#isASquareMatrix -->

    <owl:DatatypeProperty rdf:about="https://k.loria.fr/ontologies/examples/equations#isASquareMatrix">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://k.loria.fr/ontologies/examples/equations#Equation -->

    <owl:Class rdf:about="https://k.loria.fr/ontologies/examples/equations#Equation">
        <owl:disjointWith rdf:resource="https://k.loria.fr/ontologies/examples/equations#SolutionSet"/>
    </owl:Class>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#Equation1RealUnknown -->

    <owl:Class rdf:about="https://k.loria.fr/ontologies/examples/equations#Equation1RealUnknown">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://k.loria.fr/ontologies/examples/equations#Equation"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasUnknown"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                        <owl:onClass>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasType"/>
                                <owl:hasValue>real</owl:hasValue>
                            </owl:Restriction>
                        </owl:onClass>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#Equation2ndDegree -->

    <owl:Class rdf:about="https://k.loria.fr/ontologies/examples/equations#Equation2ndDegree">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://k.loria.fr/ontologies/examples/equations#PolynomialEquation"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#degree"/>
                        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</owl:hasValue>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#Equation2ndDegreeObviousRoots -->

    <owl:Class rdf:about="https://k.loria.fr/ontologies/examples/equations#Equation2ndDegreeObviousRoots">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://k.loria.fr/ontologies/examples/equations#SolutionSet"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasSolvingMethod"/>
                        <owl:hasValue rdf:resource="https://k.loria.fr/ontologies/examples/equations#findObviousRootEqDeg2"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment>DatatypeProperty annotations are essential to understand which property has to be set when the call create an instance.
Right now, it only works with one datatypeProperty per class. (With multiple ones, it&apos;s difficult to know which one relates to which result of the called function)</rdfs:comment>
        <call:hasDatatypeProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasRoot"/>
    </owl:Class>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#Equation2ndDegreeRealRoots -->

    <owl:Class rdf:about="https://k.loria.fr/ontologies/examples/equations#Equation2ndDegreeRealRoots">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://k.loria.fr/ontologies/examples/equations#SolutionSet"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasSolvingMethod"/>
                        <owl:hasValue rdf:resource="https://k.loria.fr/ontologies/examples/equations#useDiscriminant"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <call:hasDatatypeProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasRoot"/>
    </owl:Class>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#EquationSolving -->

    <owl:Class rdf:about="https://k.loria.fr/ontologies/examples/equations#EquationSolving">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://k.loria.fr/ontologies/examples/equations#Problem"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasEquation"/>
                        <owl:someValuesFrom rdf:resource="https://k.loria.fr/ontologies/examples/equations#Equation"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#FindingEigenValues -->

    <owl:Class rdf:about="https://k.loria.fr/ontologies/examples/equations#FindingEigenValues">
        <rdfs:subClassOf rdf:resource="https://k.loria.fr/ontologies/examples/equations#Problem"/>
    </owl:Class>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#Matrix -->

    <owl:Class rdf:about="https://k.loria.fr/ontologies/examples/equations#Matrix"/>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#Polynomial -->

    <owl:Class rdf:about="https://k.loria.fr/ontologies/examples/equations#Polynomial">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasExpression"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <call:hasDatatypeProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasExpression"/>
    </owl:Class>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#PolynomialEquation -->

    <owl:Class rdf:about="https://k.loria.fr/ontologies/examples/equations#PolynomialEquation">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://k.loria.fr/ontologies/examples/equations#Equation1RealUnknown"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#isAPolynomialEquation"/>
                        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#Problem -->

    <owl:Class rdf:about="https://k.loria.fr/ontologies/examples/equations#Problem">
        <owl:disjointWith rdf:resource="https://k.loria.fr/ontologies/examples/equations#SolutionSet"/>
    </owl:Class>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#RealUnknown -->

    <owl:Class rdf:about="https://k.loria.fr/ontologies/examples/equations#RealUnknown">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://k.loria.fr/ontologies/examples/equations#Unknown"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasType"/>
                        <owl:hasValue>real</owl:hasValue>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://k.loria.fr/ontologies/examples/equations#Unknown"/>
    </owl:Class>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#Solution -->

    <owl:Class rdf:about="https://k.loria.fr/ontologies/examples/equations#Solution"/>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#SolutionSet -->

    <owl:Class rdf:about="https://k.loria.fr/ontologies/examples/equations#SolutionSet">
        <rdfs:subClassOf rdf:resource="https://k.loria.fr/ontologies/examples/equations#Solution"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasRoot"/>
                        <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasRoot"/>
                        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasRoot"/>
                        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:maxCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment>represents the roots of a second degree equation. (depends on the used method)

Warning, this isn&apos;t the class representing the solutions of the problem of solving a second degree equation (which is the Solution class).</rdfs:comment>
    </owl:Class>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#SquareMatrix -->

    <owl:Class rdf:about="https://k.loria.fr/ontologies/examples/equations#SquareMatrix">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://k.loria.fr/ontologies/examples/equations#Matrix"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#isASquareMatrix"/>
                        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://k.loria.fr/ontologies/examples/equations#Matrix"/>
    </owl:Class>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#Unknown -->

    <owl:Class rdf:about="https://k.loria.fr/ontologies/examples/equations#Unknown">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasName"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://k.loria.fr/ontologies/examples/equations#callDegree -->

    <owl:NamedIndividual rdf:about="https://k.loria.fr/ontologies/examples/equations#callDegree">
        <call:functionToCall rdf:resource="https://k.loria.fr/ontologies/examples/equations#funDegree"/>
        <call:hasParams rdf:resource="https://k.loria.fr/ontologies/examples/equations#equationsCallParam1"/>
        <call:domain rdf:resource="https://k.loria.fr/ontologies/examples/equations#PolynomialEquation"/>
        <call:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <call:subsumingProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#degree"/>
    </owl:NamedIndividual>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#callFindObviousRootEqDeg2 -->

    <owl:NamedIndividual rdf:about="https://k.loria.fr/ontologies/examples/equations#callFindObviousRootEqDeg2">
        <call:functionToCall rdf:resource="https://k.loria.fr/ontologies/examples/equations#findObviousRootEqDeg2"/>
        <call:hasParams rdf:resource="https://k.loria.fr/ontologies/examples/equations#equationsCallParam1"/>
        <call:domain rdf:resource="https://k.loria.fr/ontologies/examples/equations#Equation2ndDegree"/>
        <call:range rdf:resource="https://k.loria.fr/ontologies/examples/equations#Equation2ndDegreeObviousRoots"/>
        <call:subsumingProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasSolutionSet"/>
    </owl:NamedIndividual>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#callGetCharacteristicPol -->

    <owl:NamedIndividual rdf:about="https://k.loria.fr/ontologies/examples/equations#callGetCharacteristicPol">
        <call:functionToCall rdf:resource="https://k.loria.fr/ontologies/examples/equations#funComputeCharPol"/>
        <call:hasParams rdf:resource="https://k.loria.fr/ontologies/examples/equations#matrixCallParam"/>
        <call:domain rdf:resource="https://k.loria.fr/ontologies/examples/equations#SquareMatrix"/>
        <call:range rdf:resource="https://k.loria.fr/ontologies/examples/equations#Polynomial"/>
        <call:subsumingProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasCharacteristicPolynomial"/>
    </owl:NamedIndividual>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#callIsAPolynomialEquation -->

    <owl:NamedIndividual rdf:about="https://k.loria.fr/ontologies/examples/equations#callIsAPolynomialEquation">
        <call:functionToCall rdf:resource="https://k.loria.fr/ontologies/examples/equations#funIsAPolynomialEquation"/>
        <call:hasParams rdf:resource="https://k.loria.fr/ontologies/examples/equations#equationsCallParam1"/>
        <call:domain rdf:resource="https://k.loria.fr/ontologies/examples/equations#Equation1RealUnknown"/>
        <call:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <call:subsumingProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#isAPolynomialEquation"/>
    </owl:NamedIndividual>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#callIsASquareMatrix -->

    <owl:NamedIndividual rdf:about="https://k.loria.fr/ontologies/examples/equations#callIsASquareMatrix">
        <call:functionToCall rdf:resource="https://k.loria.fr/ontologies/examples/equations#funIsASquareMatrix"/>
        <call:hasParams rdf:resource="https://k.loria.fr/ontologies/examples/equations#matrixCallParam"/>
        <call:domain rdf:resource="https://k.loria.fr/ontologies/examples/equations#Matrix"/>
        <call:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <call:subsumingProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#isASquareMatrix"/>
    </owl:NamedIndividual>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#callUseDiscriminant -->

    <owl:NamedIndividual rdf:about="https://k.loria.fr/ontologies/examples/equations#callUseDiscriminant">
        <call:functionToCall rdf:resource="https://k.loria.fr/ontologies/examples/equations#useDiscriminant"/>
        <call:hasParams rdf:resource="https://k.loria.fr/ontologies/examples/equations#equationsCallParam1"/>
        <call:domain rdf:resource="https://k.loria.fr/ontologies/examples/equations#Equation2ndDegree"/>
        <call:range rdf:resource="https://k.loria.fr/ontologies/examples/equations#Equation2ndDegreeRealRoots"/>
        <call:subsumingProperty rdf:resource="https://k.loria.fr/ontologies/examples/equations#hasSolutionSet"/>
    </owl:NamedIndividual>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#eq1 -->

    <owl:NamedIndividual rdf:about="https://k.loria.fr/ontologies/examples/equations#eq1">
        <rdf:type rdf:resource="https://k.loria.fr/ontologies/examples/equations#Equation1RealUnknown"/>
        <hasUnknown rdf:resource="https://k.loria.fr/ontologies/examples/equations#unk1"/>
        <hasEquality>x ** 2 + 2 = -3 * x</hasEquality>
    </owl:NamedIndividual>
    


    <!-- https://k.loria.fr/ontologies/examples/equations#eq2 -->

    <owl:NamedIndividual rdf:about="https://k.loria.fr/ontologies/examples/equations#eq2">
        <rdf:type rdf:resource="https://k.loria.fr/ontologies/examples/equations#Equation1RealUnknown"/>
        <hasUnknown rdf:resource="https://k.loria.fr/ontologies/examples/equations#unk2"/>
        <hasEquality>t ** 2 + 4 * t + 1 = 0</hasEquality>
