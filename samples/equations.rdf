    <!-- https://k.loria.fr/ontologies/examples/equations#callIsASquareMatrix -->

    <owl:NamedIndividual rdf:about="https://k.loria.fr/ontologies/examples/equations#callIsASquareMatrix">
        <rdf:type rdf:resource="https://k.loria.fr/ontologies/call#CallFormula"/>
        <call:hasFunctionCall rdf:resource="https://k.loria.fr/ontologies/examples/equations#funCallIsASquareMatrix"/>
        <call:domain rdf:resource="https://k.loria.fr/ontologies/examples/equations#Matrix"/>
        <call:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
