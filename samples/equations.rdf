        <call:hasPyExec>def poly_roots(unk_name: str, equality: str):
    import sympy as sp
    eq_str = equality.split(&quot;=&quot;)
    if len(eq_str) != 2:
        return None
    
    eq = sp.Eq(sp.simplify(eq_str[0]), sp.simplify(eq_str[1])).simplify().as_poly()
    roots =sp.roots(eq)
    result = None
    if len(roots) &lt; 2:
        result = {*roots}
    elif len(roots) == 2:
        l = list(roots)
        result = {l[0], l[1]} if l[0] &lt;= l[1] else {l[1], l[0]}
        return 0, [(0, &quot;hasValue&quot;, str(result))]
    else:
        return None</call:hasPyExec>
