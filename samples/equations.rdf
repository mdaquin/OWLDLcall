    <!-- https://k.loria.fr/ontologies/examples/equations#callUseDiscriminant -->

    <owl:NamedIndividual rdf:about="https://k.loria.fr/ontologies/examples/equations#callUseDiscriminant">
        <rdf:type rdf:resource="https://k.loria.fr/ontologies/call#CallFormula"/>
        <call:hasFunctionCall rdf:resource="https://k.loria.fr/ontologies/examples/equations#funCallUseDiscriminantList"/>
